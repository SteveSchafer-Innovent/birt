<!-- Written by Eclipse BIRT 1.0 -->
<report xmlns=" http://www.eclipse.org/birt/2005/design" version="1">
    <styles>
        <style name="My Style"/>
    </styles>
    <body>
        <free-form>
            <report-items>
                <label>
                    <property name="x">1mm</property>
                    <property name="y">2mm</property>
                    <property name="height">3mm</property>
                    <property name="width">4mm</property>
                    <text-property name="text">Customer Name</text-property>
                </label>
                <label>
                    <property name="x">1.1mm</property>
                    <property name="y">2.2mm</property>
                    <property name="height">3.3mm</property>
                    <property name="width">4.4mm</property>
                    <text-property name="text">Address</text-property>
                </label>
                <data>
                    <property name="distinct">repeat</property>
                    <property name="distinctReset">foo</property>
                    <expression name="valueExpr">[price] * [quan]</expression>
                    <text-property name="helpText">This is a data item.</text-property>
                </data>
            </report-items>
        </free-form>
    </body>
</report>
