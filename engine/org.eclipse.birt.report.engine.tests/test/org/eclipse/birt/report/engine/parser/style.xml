<!-- Written by Eclipse BIRT 0.0 (http://www.eclipse.com) -->
<report xmlns="http://www.eclipse.com/schemas/BIRT_schema.xsd">
    <property name="units">cm</property>
    <styles>
        <style name="Grid Style">
            <property name="color">red</property>
            <property name="borderBottomStyle">solid</property>
        </style>
        <style name="Column Style">
            <property name="color">yellow</property>
        </style>
        <style name="Row Style">
            <property name="color">blue</property>
            <property name="borderBottomStyle">double</property>
        </style>
        <style name="Cell Style">
            <property name="color">green</property>
            <property name="borderBottomStyle">none</property>
        </style>
    </styles>
    <page-setup>
        <simple-master-page name="pp">
            <property name="type">a4</property>
        </simple-master-page>
    </page-setup>
    <body>
        <grid>
			<property name="style">Grid Style</property>
            <column>
                <property name="style">Column Style</property>
            </column>
            <row>
                <property name="style">Row Style</property>
                <cell>
                    <property name="style">Cell Style</property>
                    <property name="fontSize">7pt</property>
                    <property name="color">gray</property>
                    <label>
                        <text-property name="text">1</text-property>
                    </label>
                </cell>
            </row>
        </grid>
    </body>
</report>
