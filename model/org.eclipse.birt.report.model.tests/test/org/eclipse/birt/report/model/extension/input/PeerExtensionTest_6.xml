<report xmlns="http://www.eclipse.org/birt/2005/design" version="3.2.20">
    <property name="author">asdf</property>
    <property name="createdBy">Eclipse BIRT Designer Version 2.2.0.qualifier Build &lt;@BUILD@></property>
    <property name="units">in</property>
    <styles>
    	<style name="My-Style">
            <property name="fontSize">small</property>            
        </style>
        <style name="testing-box">        	
            <property name="fontSize">medium</property>
            <property name="fontVariant">small-caps</property>
            <property name="fontWeight">200</property>
            <property name="fontStyle">italic</property>
            <property name="color">blue</property>
        </style>
        <style name="testing-box-detail">
            <property name="fontStyle">oblique</property>
            <property name="color">red</property>
        </style>
        <style name="testing-matrix">
            <property name="fontWeight">400</property>
            <property name="fontSize">x-small</property>
        </style>
        <style name="testPredefinedStyle">
            <property name="fontWeight">900</property>
            <property name="fontFamily">fantasy</property>            
        </style>
    </styles>
    <body>
        <extended-item extensionName="TestingBox" name="testBox">
        	<property name="dataSet">firstDataSet</property>
            <property name="x">1.2mm</property>
            <property name="y">11.2mm</property>
            <property name="header">
            	<extended-item extensionName="TestingMatrix" name="headerMatrix">
            	</extended-item>
            </property>
            <property name="detail">
            	<extended-item extensionName="TestingMatrix" name="detailMatrix">
            	</extended-item>
            	<extended-item extensionName="TestingMatrix" name="detailMatrixOne">
            		<property name="style">My-Style</property>
            	</extended-item>
            </property>
            <property name="footer">
            	<grid name="footerGrid">
            	</grid>
            </property>
        </extended-item>
    </body>
</report>
