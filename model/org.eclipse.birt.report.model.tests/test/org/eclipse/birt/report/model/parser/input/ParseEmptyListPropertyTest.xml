<report xmlns="http://www.eclipse.org/birt/2005/design" version="3.2.20">
	<list-property name="includeScripts" isEmpty="true"/>
	<list-property name="includeLibraries" isEmpty="true"/>
	<list-property name="cssStyleSheets" isEmpty="true"/>
    <parameters>
	    <scalar-parameter name="Param 1">
            <simple-property-list name="defaultValue"/>
            <list-property name="selectionList"/>
        </scalar-parameter>
        <scalar-parameter name="Param 2">
        	<list-property name="selectionList" isEmpty="true"/>
        	<simple-property-list name="defaultValue" isEmpty="true"/>
        </scalar-parameter>
    </parameters>
    <styles>
		<style name="style1">
            <list-property name="highlightRules"/>
            <list-property name="mapRules"/>
        </style>
        <style name="style2">
            <list-property name="highlightRules" isEmpty="true"/>
            <list-property name="mapRules" isEmpty="true"/>
        </style>
    </styles>
    <body>
    	<data name="data1">
			<list-property name="boundDataColumns">
				<structure>
					<property name="name">First data value</property>
					<list-property name="arguments" isEmpty="true"/>
					<simple-property-list name="aggregateOn" isEmpty="true"/>
				</structure>
				<structure>
					<property name="name">Second data value</property>
					<list-property name="arguments"/>
					<simple-property-list name="aggregateOn"/>
				</structure>
			</list-property>
		</data>
		<data name="data2">
			<list-property name="boundDataColumns"/>
			<list-property name="highlightRules"/>
			<list-property name="mapRules"/>
		</data>
		<data name="data3">
			<list-property name="boundDataColumns" isEmpty="true"/>
			<list-property name="highlightRules" isEmpty="true"/>
			<list-property name="mapRules" isEmpty="true"/>
		</data>
	</body>
</report>
