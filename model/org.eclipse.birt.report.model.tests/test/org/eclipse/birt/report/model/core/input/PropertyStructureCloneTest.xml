            <property name="columns">1</property>
            <property name="columnSpacing">0.25mm</property>
        </graphic-master-page>
    </page-setup>
    <body>
        <image name="Image1">
        	<property name="source">url</property>
            <expression name="uri">http://www.eclipse.org/birt/test/1.jpg</expression>
