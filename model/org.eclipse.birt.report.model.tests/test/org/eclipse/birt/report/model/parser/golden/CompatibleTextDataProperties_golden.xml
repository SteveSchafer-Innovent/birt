<report xmlns="http://www.eclipse.org/birt/2005/design" version="3.2.23" id="1">
    <property name="layoutPreference">auto layout</property>
    <styles>
        <style name="My-Style" id="2"/>
    </styles>
    <page-setup>
        <graphic-master-page name="My Page" id="3">
            <property name="topMargin">1in</property>
            <property name="leftMargin">1.25in</property>
            <property name="bottomMargin">1in</property>
            <property name="rightMargin">1.25in</property>        
            <contents>
                <label name="label1" id="4">
                    <text-property name="text">First Page</text-property>
                </label>
            </contents>
        </graphic-master-page>
    </page-setup>
    <body>
        <text-data name="Multi Line Data" id="5">
            <expression name="valueExpr">value expr</expression>
            <property name="contentType">content type expr</property>
        </text-data>
        <text-data name="Text Data" id="6">
            <property name="contentType">plain</property>
        </text-data>
    </body>
</report>
